{
  "name": "fluence",
  "namespace": "fluence",
  "version": "0.3.6",
  "description": "Fluence Javascript Client allows you to interact with a Fluence real-time cluster for decentralized computations.",
  "main": "./dist/fluence.js",
  "typings": "./dist/fluence.d.ts",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "build": "webpack",
    "package:build": "tsc",
    "start:dev": "webpack-dev-server",
    "typechain:generate": "typechain --target=web3-1.0.0 '../bootstrap/contracts/compiled/*.abi'",
    "prepare": "npm run compile-sol --prefix ../bootstrap/ && npm run typechain:generate && cp ../bootstrap/contracts/compiled/Network.abi src/Network.json && tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fluencelabs/fluence.git"
  },
  "homepage": "https://github.com/fluencelabs/fluence",
  "bugs": {
    "url": "https://github.com/fluencelabs/fluence/issues"
  },
  "keywords": [
    "fluence",
    "javascript",
    "API",
    "decentralized",
    "database"
  ],
  "author": "Fluence Labs",
  "authors": [
    {
      "name": "Dmitry Shakhtarin",
      "email": "dmitry.shakhtarin@fluence.one",
      "url": "https://github.com/DieMyst"
    },
    {
      "name": "folex",
      "email": "alexey@fluence.one",
      "url": "https://github.com/folex"
    }
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/base64-js": "1.2.5",
    "@types/chai": "4.1.7",
    "@types/debug": "^4.1.4",
    "@types/elliptic": "6.4.2",
    "@types/mocha": "5.2.5",
    "@types/promise.prototype.finally": "2.0.2",
    "@types/randomstring": "1.1.6",
    "@types/secp256k1": "3.5.0",
    "@types/sha256": "0.2.0",
    "@types/web3": "1.0.18",
    "awesome-typescript-loader": "5.2.1",
    "chai": "4.2.0",
    "clean-webpack-plugin": "0.1.19",
    "html-webpack-plugin": "3.2.0",
    "mocha": "5.2.0",
    "ts-mockito": "2.3.1",
    "ts-node": "7.0.1",
    "typechain": "0.3.10",
    "typescript": "3.2.4",
    "webpack": "4.29.0",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14"
  },
  "dependencies": {
    "array-buffer-to-hex": "^1.0.0",
    "axios": "0.18.0",
    "base64-js": "1.3.0",
    "debug": "4.1.1",
    "hex-to-array-buffer": "^1.1.0",
    "randomstring": "1.1.5",
    "secp256k1": "3.6.2",
    "sha256": "0.2.0",
    "ts-option": "1.1.5",
    "web3": "1.2.1"
  }
}
